extends default

block header

block content
  div.profile
    div.profile-image(style=`background: url('/images/${dance.imgSrc}') top center
    no-repeat; background-size: contain;`)
    div.profile-details
      h1.profile-name
        | #{dance.name}
      h2.profile-tagline
        | #{dance.tagline}
      p.profile-bio
        | #{dance.bio}
